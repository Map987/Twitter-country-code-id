{
    "name": "__MSG_Name__",
    "short_name": "xToTwitter",
    "version": "1.4.0",
    "manifest_version": 3,
    "description": "__MSG_Description__",
    "default_locale": "en",
    "icons": {
      "16": "assets/icon16.png",
      "48": "assets/icon48.png",
      "128": "assets/icon128.png"
    },
    "content_scripts": [
        {
          "matches": ["https://twitter.com/*", "https://X.com/*"],
          "run_at": "document_start",
          "js": ["contentScript.js"]
        }
    ]
}